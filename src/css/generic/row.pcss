  }

  &._col8 {
    @apply --col-8;
  }

  &._col9 {
    @apply --col-9;
  }

  &._col10 {
    @apply --col-10;
  }

  &._col11 {
    @apply --col-11;
  }

  &._col12 {
    @apply --col-12;
  }

  /* col offsets */

  &.-colLeft0 {
    margin-left: 0;
  }

  &.-colLeft1 {
    margin-left: var(--1-col-pct);
  }

  &.-colLeft2 {
    margin-left: var(--2-col-pct);
  }

  &.-colLeft3 {
    margin-left: var(--3-col-pct);
  }

  &.-colLeft4 {
