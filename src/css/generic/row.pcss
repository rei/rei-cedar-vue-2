********************/

.cdr-row {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin-left: calc(var(--rowGutter) * -1 / 2);
  margin-right: calc(var(--rowGutter) * -1 / 2);

  &.cdr-col {
    margin: 0;
    padding: 0;

    &:not([class*="col-0"]) {
      display: flex;
    }
  }

  /* row by number */

  &._row1 {
    & > .cdr-col:not([class*="_col"]) {
      @apply --row-1;
    }
  }

  &._row2 {
    & > .cdr-col:not([class*="_col"]) {
