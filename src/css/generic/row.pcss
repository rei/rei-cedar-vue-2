  }

  &.-colLeft11 {
    margin-left: var(--11-col-pct);
  }

  &.-colRight0 {
    margin-right: 0;
  }

  &.-colRight1 {
    margin-right: var(--1-col-pct);
  }

  &.-colRight2 {
    margin-right: var(--2-col-pct);
