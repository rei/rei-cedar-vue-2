/*******************
  col classes
********************/

.cdr-col {
  flex: 1 1 auto;
  padding-left: calc(var(--rowGutter) / 2);
  padding-right: calc(var(--rowGutter) / 2);
  margin-bottom: var(--colBottom);
  display: flex;

  &__content {
    width: 100%;
    height: 100%;
  }

  /* col by number */

  &._col1 {
    @apply --col-1;
  }

  &._col2 {
    @apply --col-2;
  }

  &._col3 {
    @apply --col-3;
  }

  &._col4 {
    @apply --col-4;
  }

  &._col5 {
    @apply --col-5;
  }

  &._col6 {
    @apply --col-6;
  }

  &._col7 {
