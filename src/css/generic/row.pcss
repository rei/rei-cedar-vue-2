:root {
  --rowGutter: calc(var(--container-x-padding) * 2);
  --colBottom: 10px;
  --rowColumns: 12;
  --1-row-pct: calc(var(--rowColumns) / 1 / var(--rowColumns) * 100%);
  --2-row-pct: calc(var(--rowColumns) / 2 / var(--rowColumns) * 100%);
  --3-row-pct: calc(var(--rowColumns) / 3 / var(--rowColumns) * 100%);
  --4-row-pct: calc(var(--rowColumns) / 4 / var(--rowColumns) * 100%);
  --5-row-pct: calc(var(--rowColumns) / 5 / var(--rowColumns) * 100%);
  --6-row-pct: calc(var(--rowColumns) / 6 / var(--rowColumns) * 100%);
  --7-row-pct: calc(var(--rowColumns) / 7 / var(--rowColumns) * 100%);
  --8-row-pct: calc(var(--rowColumns) / 8 / var(--rowColumns) * 100%);
  --9-row-pct: calc(var(--rowColumns) / 9 / var(--rowColumns) * 100%);
  --10-row-pct: calc(var(--rowColumns) / 10 / var(--rowColumns) * 100%);
  --11-row-pct: calc(var(--rowColumns) / 11 / var(--rowColumns) * 100%);
  --12-row-pct: calc(var(--rowColumns) / 12 / var(--rowColumns) * 100%);
  --row-1: {
    flex-basis: var(--1-row-pct);
    width: var(--1-row-pct);
    max-width: var(--1-row-pct);
  }
  --row-2: {
    flex-basis: var(--2-row-pct);
    width: var(--2-row-pct);
    max-width: var(--2-row-pct);
  }
  --row-3: {
    flex-basis: var(--3-row-pct);
    width: var(--3-row-pct);
    max-width: var(--3-row-pct);
  }
