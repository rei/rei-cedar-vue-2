  --8-col-pct: calc(8 / var(--rowColumns) * 100%);
  --9-col-pct: calc(9 / var(--rowColumns) * 100%);
  --10-col-pct: calc(10 / var(--rowColumns) * 100%);
  --11-col-pct: calc(11 / var(--rowColumns) * 100%);
  --12-col-pct: calc(12 / var(--rowColumns) * 100%);
