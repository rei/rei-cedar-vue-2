    "svg-sprite": "^1.5.0",
    "tinycolor2": "^1.4.1",
    "vue": "^2.5.22",
    "vue-router": "^3.0.2",
    "vue-runtime-helpers": "^1.0.1",
    "vue-template-compiler": "^2.5.22"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 6.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 11"
  ],
  "config": {
