{
  "name": "@rei/cedar",
  "version": "8.0.0-alpha.1",
  "description": "REI Cedar Component Library",
  "homepage": "https://rei.github.io/rei-cedar/",
  "license": "MIT",
  "private": false,
  "sideEffects": [
    "*.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "REI Software Engineering",
  "main": "dist/cedar.js",
  "module": "dist/lib/src/index.mjs",
  "style": "dist/style/cedar-full.css",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rei/rei-cedar.git"
  },
  "scripts": {
    "prepublishOnly": "npm-run-all lint build",
    "dev:build": "npm run build:cssfonts && npm run build:icons && cross-env NODE_ENV=dev rollup -c rollup.config.dev.js",
    "dev": "npm run dev:build -- -w",
    "build": "npm-run-all clean build:cssfonts build:icons build:cjs build:esm build:extractcss build:cssdocs",
    "build:cjs": "cross-env NODE_ENV=prod BABEL_ENV=cjs rollup -c rollup.config.js",
    "build:esm": "cross-env NODE_ENV=prod BABEL_ENV=esm rollup -c rollup.config.js",
    "unit": "jest",
    "e2e": "cross-env NODE_ENV=test rollup -c rollup.config.dev.js && node test/e2e/runner.js",
    "test": "npm-run-all unit e2e",
    "clean": "rm -rf dist",
    "lint": "npm-run-all lint:js lint:styles",
    "lint:js": "eslint --ext .js,.vue src",
    "lint:styles": "stylelint src/**/*.scss",
    "prepush": "#npm-run-all lint unit",
    "reference": "backstop reference --configPath=backstop.js",
    "compare": "backstop test --configPath=backstop.js",
    "approve": "backstop approve --configPath=backstop.js",
    "create": "node generator.js",
    "changelog": "standard-changelog",
    "prerelease": "npm-publish-prerelease",
    "build:variables": "node build/component-variables-transfer.js",
    "build:extractcss": "node build/extract-css.js",
    "build:cssdocs": "node build/cssdoc.js",
    "build:cssfonts": "node build/cdr-fonts.js",
    "build:icons": "node src/components/icon/build/generate.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.10",
    "@betit/rollup-plugin-rename-extensions": "^0.1.0",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^11.0.0",
    "@rei/cdr-tokens": "^7.0.1",
    "@rei/cedar-icons": "^2.1.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/test-utils": "^2.0.0-beta.13",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-istanbul": "^6.0.0",
    "backstopjs": "^5.0.7",
    "chalk": "^4.1.0",
    "chromedriver": "^87.0.0",
    "codecov": "^3.8.1",
    "concat": "^1.0.3",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-tree-shaking": "^1.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "fs-extra": "^9.0.0",
    "function-bind": "^1.1.0",
    "glob": "^7.1.6",
    "husky": "^4.3.6",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^7.3.3",
    "jest": "^26.6.3",
    "jest-css-modules-transform": "^4.1.0",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "klaw-sync": "^6.0.0",
    "lodash": "^4.17.20",
    "nightwatch": "^1.5.1",
    "nightwatch-axe": "^0.1.5",
    "node-cmd": "^3.0.0",
    "node-sass": "^4.14.1",
    "node-sass-package-importer": "^5.3.2",
    "npm-publish-prerelease": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.0.0",
    "postcss-calc": "^7.0.5",
    "postcss-import": "^14.0.0",
    "postcss-inline-svg": "^5.0.0",
    "postcss-modules": "^4.0.0",
    "postcss-pxtorem": "cowills/postcss-pxtorem#postcss-8-update",
    "postcss-scss": "^3.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.35.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-multi-entry": "^2.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-vue": "^6.0.0",
    "sassdoc": "^2.7.3",
    "serve-handler": "^6.1.3",
    "sinon": "^9.2.2",
    "standard-changelog": "^2.0.27",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.18.0",
    "tinycolor2": "^1.4.2",
    "typescript": "^4.1.3",
    "vue": "^3.0.5",
    "vue-router": "^4.0.2"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 6.0.0"
  },
  "browserslist": [
    "Chrome >= 77",
    "Firefox >= 70",
    "iOS > 11",
    "Safari >= 9",
    "Edge >= 17"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run prepush"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@babel/runtime-corejs3": "^7.12.5",
    "clsx": "^1.1.1",
    "lodash-es": "^4.17.20",
    "tabbable": "^4.0.0"
  }
}
