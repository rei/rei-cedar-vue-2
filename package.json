{
  "name": "@rei/cedar",
  "version": "2.0.1",
  "description": "REI Cedar Component Library",
  "homepage": "https://rei.github.io/rei-cedar/",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "author": "REI Software Engineering",
  "main": "dist/cedar.cjs.js",
  "module": "dist/cedar.esm.js",
  "style": "dist/cedar.css",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rei/rei-cedar.git"
  },
  "scripts": {
    "prepublishOnly": "npm run lint && npm run build",
    "dev": "cross-env NODE_ENV=dev rollup -w -c rollup.config.dev.js",
    "build": "npm run clean && npm run build:csr && npm run build:ssr && node build/extract-css.js",
    "build:csr": "cross-env NODE_ENV=prod rollup -c rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=prod SSR_ENV=ssr rollup -c rollup.config.js",
    "unit": "cross-env NODE_ENV=test rollup -c rollup.config.unit.js && jest",
    "e2e": "rollup -c rollup.config.dev.js && node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "clean": "rm -rf dist",
    "lint": "npm run lint:js && npm run lint:styles",
    "lint:js": "eslint --ext .js,.vue src",
    "lint:styles": "stylelint src",
    "reference": "backstop reference --configPath=backstop.js",
    "compare": "backstop test --configPath=backstop.js",
    "approve": "backstop approve --configPath=backstop.js",
    "create": "node generator.js",
    "changelog": "standard-changelog",
    "prerelease": "npm-publish-prerelease"
  },
  "devDependencies": {
