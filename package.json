{
  "name": "rei-cedar",
  "version": "5.0.0-0",
  "description": "REI Cedar Style Framework",
  "homepage": "https://rei.github.io/rei-cedar/",
  "license": "MIT",
  "author": "REI Software Engineering",
  "private": true,
  "scripts": {
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "dev": "node build/dev-server.js",
    "build": "cross-env NODE_ENV=production node build/build.js",
    "unit": "cross-env NODE_ENV=test nyc --check-coverage --lines 100 --reporter=lcov --report-dir=test/unit/coverage --reporter=text mocha-webpack --webpack-config build/webpack.test.conf.js --require test/setup.js src/**/*.spec.js",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs utils build",
    "reference": "backstop reference --configPath=backstop.js",
    "compare": "backstop test --configPath=backstop.js",
    "approve": "backstop approve --configPath=backstop.js",
    "docs": "vue-styleguidist server",
    "build:docs": "lerna run build:docs",
    "build:archive": "node build/component-docs-archive.js",
    "build:transfer": "node build/component-docs-transfer.js",
    "create": "node generator.js",
    "bootstrap": "lerna bootstrap",
    "start": "lerna bootstrap && lerna run build",
    "vue:build": "npm run vue:docs && npm run vue:archive && npm run vue:transfer",
    "changelog": "standard-changelog",
    "theo": "node utils/theo.js"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.0",
    "@commitlint/config-conventional": "^6.1.0",
    "@vue/test-utils": "^1.0.0-beta.16",
    "autoprefixer": "^8.5.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "backstopjs": "^3.1.19",
    "chalk": "^2.3.1",
    "chromedriver": "^2.35.0",
    "clean-webpack-plugin": "^0.1.16",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.4.1",
    "cross-env": "^5.0.3",
    "cross-spawn": "^6.0.4",
    "css-loader": "^0.28.11",
    "cz-conventional-changelog": "^2.1.0",
    "cz-lerna-changelog": "^1.2.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.0.1",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^23.1.0",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.8",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "fs-extra": "^6.0.1",
    "function-bind": "^1.1.0",
    "glob": "^7.1.2",
    "glob-promise": "^3.4.0",
    "html-loader": "^0.5.0",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.18.0",
    "husky": "^0.14.3",
    "inquirer": "^5.1.0",
    "jsdom": "^11.10.0",
    "jsdom-global": "^3.0.2",
    "json2md": "^1.6.1",
    "karma": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "klaw-sync": "^4.0.0",
    "lerna": "^2.10.1",
    "loader-utils": "^1.1.0",
    "lodash": "^4.17.5",
    "minimist": "^1.2.0",
    "mocha": "^5.1.1",
    "mocha-webpack": "^1.1.0",
    "nightwatch": "^0.9.19",
    "nightwatch-axe": "^0.1.5",
    "nyc": "^11.8.0",
    "opn": "^5.2.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^2.1.0",
    "phantomjs-prebuilt": "^2.1.14",
    "pkg-ok": "^2.1.0",
    "postcss": "^6.0.22",
    "postcss-calc": "^6.0.0",
    "postcss-class-prefix": "^0.3.0",
    "postcss-custom-media": "^6.0.0",
