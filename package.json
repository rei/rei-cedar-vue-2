    "postcss-simple-vars": "^4.0.0",
    "postcss-strip-inline-comments": "^0.1.5",
    "raw-loader": "^0.5.1",
    "replace-bundle-webpack-plugin": "^1.0.0",
    "rimraf": "^2.6.0",
    "selenium-server": "^3.9.1",
    "semver": "^5.5.0",
    "semver-diff": "^2.1.0",
    "sinon": "^5.0.7",
    "standard-changelog": "^1.0.12",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.1.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-webpack-plugin": "^0.10.3",
    "theo": "^7.0.1",
    "tinycolor2": "^1.4.1",
    "uglifyjs-webpack-plugin": "^1.2.0",
    "url-loader": "^1.0.1",
    "vue": "^2.5.13",
    "vue-loader": "^15.0.7",
    "vue-router": "^3.0.1",
    "vue-style-loader": "^4.1.0",
    "vue-styleguidist": "^1.4.8",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "^2.22.1",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.7.2"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 11"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-lerna-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commitmsg": "commitlint -e $GIT_PARAMS"
    }
  },
  "nyc": {
    "include": [
      "src/**/*.(js|vue)"
    ],
    "instrument": false,
    "sourceMap": false
  },
  "dependencies": {
    "rei-cedar-tokens": "git+https://github.com/rei/rei-cedar-tokens.git#0.1.4"
  }
}
