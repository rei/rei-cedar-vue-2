{
  "name": "@rei/cedar",
  "version": "3.0.1",
  "description": "REI Cedar Component Library",
  "homepage": "https://rei.github.io/rei-cedar/",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "author": "REI Software Engineering",
  "main": "dist/cedar.js",
  "module": "dist/lib/index.mjs",
  "style": "dist/cedar.css",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rei/rei-cedar.git"
  },
  "scripts": {
    "prepublishOnly": "npm-run-all lint build",
    "dev": "npm-run-all build:cssfonts && cross-env NODE_ENV=dev rollup -w -c rollup.config.dev.js",
    "build": "npm-run-all clean build:cssfonts build:cjs build:esm build:extractcss build:cssdocs",
    "build:cjs": "cross-env NODE_ENV=prod BABEL_ENV=cjs rollup -c rollup.config.js",
    "build:esm": "cross-env NODE_ENV=prod BABEL_ENV=esm rollup -c rollup.config.js",
    "unit": "jest",
    "e2e": "cross-env NODE_ENV=test rollup -c rollup.config.dev.js && node test/e2e/runner.js",
    "test": "npm-run-all unit e2e",
    "clean": "rm -rf dist",
    "lint": "npm-run-all lint:js lint:styles",
    "lint:js": "eslint --ext .js,.vue,.jsx src",
    "lint:styles": "stylelint src",
    "reference": "backstop reference --configPath=backstop.js",
    "compare": "backstop test --configPath=backstop.js",
    "approve": "backstop approve --configPath=backstop.js",
    "create": "node generator.js",
    "changelog": "standard-changelog",
    "prerelease": "npm-publish-prerelease",
    "build:variables": "node build/component-variables-transfer.js",
    "build:extractcss": "node build/extract-css.js",
    "build:cssdocs": "node build/cssdoc.js",
    "build:cssfonts": "node build/cdr-fonts.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@betit/rollup-plugin-rename-extensions": "0.0.5",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@rei/cdr-tokens": "^2.0.0",
    "@vue/babel-preset-jsx": "^1.1.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-plugin-istanbul": "^5.1.0",
    "backstopjs": "^3.8.8",
    "chalk": "^2.4.2",
    "chokidar": "^2.1.6",
    "chromedriver": "77.0.0",
    "codecov": "^3.6.1",
    "concat": "^1.0.3",
    "cross-env": "^5.2.0",
    "cross-spawn": "^6.0.4",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-tree-shaking": "^1.7.4",
    "eslint-plugin-vue": "^5.1.0",
    "expect": "^24.0.0",
    "fs-extra": "^7.0.1",
    "function-bind": "^1.1.0",
    "glob": "^7.1.3",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^6.2.1",
    "jest": "^24.7.1",
    "jest-css-modules-transform": "^3.0.1",
    "jsdom": "^13.2.0",
    "jsdom-global": "^3.0.2",
    "klaw-sync": "^6.0.0",
    "lodash": "^4.17.15",
    "mocha": "^5.1.1",
    "nightwatch": "^1.1.8",
    "nightwatch-axe": "^0.1.5",
    "node-cmd": "^3.0.0",
    "node-sass": "^4.12.0",
    "node-sass-package-importer": "^5.3.2",
    "npm-publish-prerelease": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "ora": "^3.0.0",
    "pkg-ok": "^2.3.1",
    "postcss": "^7.0.14",
    "postcss-calc": "^7.0.1",
    "postcss-import": "^12.0.1",
    "postcss-inline-svg": "^3.0.0",
    "postcss-pxtorem": "^4.0.1",
    "postcss-scss": "^2.0.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.19.4",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-analyzer": "^3.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-copy": "^2.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-livereload": "^1.0.1",
    "rollup-plugin-multi-entry": "^2.1.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-vue": "^5.0.0",
    "sassdoc": "^2.7.0",
    "serve-handler": "^6.0.0",
    "sinon": "^7.2.3",
    "standard-changelog": "^2.0.11",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-scss": "^3.6.1",
    "svg-sprite": "^1.5.0",
    "tinycolor2": "^1.4.1",
    "vue": "^2.5.22",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.5.22"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 6.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 11"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@babel/runtime-corejs3": "^7.5.5",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "clsx": "^1.0.4",
    "lodash-es": "^4.17.15"
  }
}
